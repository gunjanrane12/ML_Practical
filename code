import requests as rq




if asg.isdigit():
    asg = int(asg)
    if asg < 6:
        link = f"https://tinyurl.com/irpra/A{asg}.ipynb"
        filename = f"Untitled_a.ipynb"
    elif asg==8:
        link = f"https://tinyurl.com/bifpra/A{4}.ipynb"
        filename = f"Untitled_a.ipynb"
    else:
        link = f"https://tinyurl.com/bifpra/A{asg-5}.ipynb"
        filename = f"Untitled_a.ipynb"
else:
    link = f"https://tinyurl.com/bifpra/{asg}"
    filename = asg

a = rq.get(link)
with open(filename, "wb") as f:
    f.write(a.content)
